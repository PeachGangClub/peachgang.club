{
  "name": "peachgang.club",
  "version": "1.0.0",
  "main": "lib/index.js",
  "scripts": {
    "dev": "next",
    "build": "next build && tsc -p tsconfig_functions.json",
    "start": "next start",
    "watch": "tsc --watch -p tsconfig_functions.json",
    "serve": "npm run build && npx tsc-watch -p tsconfig_functions.json --onFirstSuccess 'firebase emulators:start --only functions,hosting'",
    "shell": "npm run build && firebase functions:shell",
    "deploy": "npm run build && firebase deploy --only functions,hosting",
    "lint": "eslint '**/*.{ts,tsx}'",
    "lint:fix": "eslint '**/*.{ts,tsx}' --fix"
  },
  "dependencies": {
    "@iconify/icons-logos": "^1.1.6",
    "@iconify/icons-twemoji": "^1.1.0",
    "@iconify/react": "^1.1.4",
    "firebase-admin": "^9.4.2",
    "firebase-functions": "^3.13.0",
    "framer-motion": "^3.1.1",
    "next": "latest",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "styled-components": "^5.2.1"
  },
  "devDependencies": {
    "@types/node": "^12.13.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "babel-plugin-styled-components": "^1.12.0",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "firebase-tools": "^9.0.1",
    "prettier": "^2.2.1",
    "tsc-watch": "^4.2.9",
    "typescript": "4.0"
  },
  "license": "MIT"
}
